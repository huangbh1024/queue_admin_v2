    "commitizen": "^4.3.0",
    "cz-git": "^1.8.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.20.1",
    "husky": "^8.0.3",
